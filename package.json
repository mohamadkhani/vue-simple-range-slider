{
    "name": "vue-simple-range-slider",
    "description": "Change Your numeric value or numeric range value with dragging handles",
    "version": "0.1.10",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/mohamadkhani/vue-simple-range-slider"
    },
    "keywords": [
        "vue",
        "range-slider",
        "numeric-picker",
        "range-picker",
        "numeric-range-picker",
        "numeric-range",
        "input-element",
        "range-input"
    ],
    "scripts": {
        "dev": "vue-demi-switch 3 vue && vite -c ./src/vue3/vite.config.ts",
        "dev:vue2": "vue-demi-switch 2 vue2 && vite -c ./src/vue2/vite.config.ts",
        "build": "vue-demi-switch 3 vue && vite -c ./src/vue3/vite.config.ts build",
        "build:vue2": "vue-demi-switch 2 vue2 && vite -c ./src/vue2/vite.config.ts build",
        "test": "vue-demi-switch 3 vue && vitest",
        "test:vue2": "vue-demi-switch 2 vue2 && vitest",
        "lint": "vue-cli-service lint",
        "test:2": "vue-demi-switch 2 vue2 && jest",
        "test:3": "vue-demi-switch 3 && jest"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@types/chai": "^4.3.1",
        "@types/jest": "^28.1.6",
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.30.7",
        "@typescript-eslint/parser": "^5.30.7",
        "@vitejs/plugin-vue": "^3.0.1",
        "@vitejs/plugin-vue2": "^1.1.2",
        "@vitest/ui": "^0.18.1",
        "@vue/compiler-sfc": "^3.2.37",
        "@vue/composition-api": "^1.7.0",
        "@vue/eslint-config-typescript": "^11.0.0",
        "@vue/reactivity": "^3.2.37",
        "@vue/test-utils": "^2.0.2",
        "chai": "^4.3.6",
        "eslint": "^8.20.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.2.0",
        "jest": "^28.1.3",
        "jsdom": "^20.0.0",
        "lerna": "^4.0.0",
        "nx": "^14.4.3",
        "prettier": "^2.7.1",
        "sass": "^1.53.0",
        "ts-jest": "^28.0.7",
        "typescript": "^4.1.5",
        "vite": "^3.0.2",
        "vitest": "^0.18.1",
        "vue": "^3.2.37",
        "vue-demi": "latest",
        "vue-jest": "^5.0.0-0",
        "vue-loader": "^17.0.0",
        "vue-template-compiler": "^2.7.7",
        "vue2": "npm:vue@^2.7.7"
    },
    "main": "dist/vueSimpleRangeSlider.common.js",
    "files": [
        "dist/*",
        "src/*",
        "public/*",
        "*.json",
        "*.js",
        "LICENSE"
    ]
}
